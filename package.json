    "vue-loader": "^15.4.2",
    "vuepress": "^0.14.4",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "last 8 versions",
    "Chrome >= 30",
    "Firefox >= 30",
    "Explorer >= 9"
