  "repository": {
    "type": "git",
    "url": "https://github.com/dbrekalo/vue-dismiss"
  },
  "license": "MIT",
  "files": [
    "/dist",
    "/src"
  ],
  "main": "dist/vueDismiss.js",
  "scripts": {
    "dev": "npm run dev:docs",
    "dev:docs": "vuepress dev documentation",
    "build": "npm run build:lib && npm run build:docs",
